{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true
  },
  "globals": {
    "twttr": true,
    "instgrm": true,
    "FB": true,
    "videojs": true,
    "ga": true,
    "_gaq": true
  },
  "rules": {
    "arrow-body-style": 0,
    "no-param-reassign": [2, { "props": false }],
    "jsx-quotes": [2, "prefer-single"],
    "space-before-function-paren": [2, "never"],
    "func-names": 0,
    "no-debugger": 0,
    "comma-dangle": 0,
    "max-len": [1, 80, 4],
    "consistent-return": 0,
    "no-use-before-define": 0,
    "semi": 1,
    "default-case": 0,
    "react/jsx-no-bind": 0,
    "no-return-assign": 0
  }
}
